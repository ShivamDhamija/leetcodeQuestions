class Solution {
public:
    vector<int> findSmallestSetOfVertices(int n, vector<vector<int>>& e) {
        vector<int>v(n);
        for(auto i:e)
        {
            v[i[1]]++;
        }
        vector<int>ans;
        for(int i=0;i<n;i++)
            if(v[i]==0)
                ans.push_back(i);
        return ans;
    }
};
